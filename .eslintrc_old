{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:import/errors"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceTypes": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "commonjs": true
    },
    "plugins": [
        "react"
    ]
}