{
  "name": "vivi",
  "version": "0.0.1",
  "description": "Ui Components for Dashboards",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir dist --ignore demo/*",
    "build:css": "postcss --use autoprefixer --use postcss-import src/style.css > dist/style.css",
    "build-webpack": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
    "lint": "eslint src",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "storybook": "start-storybook -p 9001"
  },
  "author": "Emanuele Ingrosso <ingro85@gmail.com>",
  "license": "MIT",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {},
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.3.0",
    "autoprefixer": "^6.4.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.8",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.3",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.15.0",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2",
    "react": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dates": "^1.0.0",
    "react-dom": "^15.3.1",
    "react-virtualized": "^7.19.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "lodash": "^4.0.0",
    "react": "^0.14.0 || ^15.0.0",
    "react-addons-shallow-compare": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-virtualized": "^7.0.0"
  }
}
