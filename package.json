{
  "name": "vivi",
  "version": "0.4.6",
  "description": "Ui Components for Dashboards",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir lib --ignore demo/*",
    "build:css": "postcss --use autoprefixer --use postcss-import src/style.css > lib/style.css",
    "build-webpack": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "storybook": "start-storybook -p 9001",
    "build:storybook": "build-storybook -c .storybook -o .out"
  },
  "author": "Emanuele Ingrosso <ingro85@gmail.com>",
  "license": "MIT",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-highlighter": "^0.3.3",
    "tether": "^1.3.7",
    "uncontrollable": "^4.0.3"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/storybook": "^2.29.3",
    "@kadira/storybook-addon-knobs": "^1.4.1",
    "@kadira/storybook-addon-options": "^1.0.1",
    "autoprefixer": "^6.5.2",
    "ava": "^0.16.0",
    "axios": "^0.15.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "cross-env": "^3.1.2",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^16.0.2",
    "lodash": "^4.16.6",
    "moment": "^2.15.2",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.3",
    "rc-pagination": "^1.5.5",
    "rc-tooltip": "^3.4.2",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dates": "^3.6.0",
    "react-dom": "^15.3.2",
    "react-json-pretty": "^1.2.1",
    "react-modal": "^1.5.2",
    "react-redux": "^4.4.5",
    "react-select": "^1.0.0-rc.2",
    "react-timeago": "^3.1.3",
