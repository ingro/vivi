    "@storybook/addon-options": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "autoprefixer": "^9.0.2",
    "ava": "^0.25.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
