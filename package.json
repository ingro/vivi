{
  "name": "vivi",
  "version": "0.6.2",
  "description": "Ui Components for Dashboards",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir lib --ignore demo/*",
    "build:css": "postcss src/style.css > lib/style.css --use autoprefixer --no-map --use postcss-import",
    "build-webpack": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "storybook": "start-storybook -p 9001 ",
    "build:storybook": "build-storybook -o .out"
  },
  "author": "Emanuele Ingrosso <ingro85@gmail.com>",
  "license": "MIT",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "date-fns": "^1.28.5",
    "prop-types": "^15.6.0",
    "react-highlight-words": "^0.9.0",
    "react-popper": "^0.7.3",
    "uncontrollable": "^4.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-info": "^3.2.12",
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/addon-options": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "autoprefixer": "^7.1.3",
    "ava": "^0.22.0",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
