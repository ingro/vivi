{
  "name": "vivi",
  "version": "0.6.15",
  "description": "React powered Ui Components for Dashboards",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir lib --ignore demo/*",
    "build:css": "postcss src/style.css > lib/style.css --use autoprefixer --no-map --use postcss-import",
    "build-webpack": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "storybook": "start-storybook -p 9001 ",
    "build:storybook": "build-storybook -o .out"
  },
  "author": {
    "name": "Emanuele Ingrosso",
    "email": "ingro85@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ingro/vivi.git"
  },
  "bugs": {
    "url": "https://github.com/ingro/vivi/issues"
  },
  "homepage": "https://github.com/ingro/vivi",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^1.29.0",
    "prop-types": "^15.6.2",
    "react-highlight-words": "^0.12.0",
    "react-popper": "^1.0.0",
    "uncontrollable": "^4.1.0"
  },
