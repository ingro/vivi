    "rc-tooltip": "^3.0.0",
    "react": "^0.14.0 || ^15.0.0",
    "react-addons-shallow-compare": "^0.14.0 || ^15.0.0",
    "react-dates": "^3.6.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-select": "^1.0.0-rc.2",
    "react-virtualized": "^8.0.0"
  }
}
