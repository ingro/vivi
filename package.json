    "lodash": "^4.0.0",
    "rc-pagination": "^1.0.0",
    "rc-tooltip": "^3.0.0",
    "react": "^0.14.0 || ^15.0.0",
    "react-addons-shallow-compare": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-select": "^1.0.0-rc.3",
    "react-virtualized": "^9.0.0"
  }
}
