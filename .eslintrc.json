{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:import/errors"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceTypes": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "plugins": [
        "react"
    ]
}